{
    "npm-modules": {
        "key": "./npm-shrinkwrap.canonical.json",
        "cache": "../node_modules",
        "generate": "./uncached-npm-install.sh"
    },
    "rubygems": {
        "key": "./Gemfile.lock",
        "cache": "$GEMDIR",
        "generate": "bundle install --gemfile=../Gemfile"
    }
}
